KERNEL=="ttyMCC", MODE="0660", GROUP="dialout", SYMLINK+="ttyS0"
KERNEL=="ttymxc0",  MODE="0660", GROUP="dialout"
KERNEL=="ttymxc1",  MODE="0660", GROUP="dialout"
KERNEL=="ttymxc5",  MODE="0660", GROUP="dialout"

SUBSYSTEM=="gpio", KERNEL=="gpiochip*", ACTION=="add", PROGRAM="/bin/sh -c 'chown root:dialout /sys/class/gpio/export /sys/class/gpio/unexport ; chmod 220 /sys/class/gpio/export /sys/class/gpio/unexport'"
SUBSYSTEM=="gpio", KERNEL=="gpio*", ACTION=="add", PROGRAM="/bin/sh -c 'chown root:dialout /sys%p/active_low /sys%p/direction /sys%p/edge /sys%p/value ; chmod 660 /sys%p/active_low /sys%p/direction /sys%p/edge /sys%p/value'"
SUBSYSTEM=="misc", KERNEL=="Freescale*", ACTION=="add", PROGRAM="/bin/sh -c 'chown root:dialout /sys%p/enable /sys%p/data /sys%p/poll_delay /sys%p/position /sys/devices/virtual/misc/FreescaleAccelerometer/motion_detect ; chmod 660 /sys%p/enable /sys%p/data /sys%p/poll_delay /sys%p/position /sys/devices/virtual/misc/FreescaleAccelerometer/motion_detect'"
